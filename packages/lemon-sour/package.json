{
  "name": "lemon-sour",
  "version": "1.1.0",
  "description": "> TODO: description",
  "author": "hisasann <hisasann25@gmail.com>",
  "homepage": "https://github.com/lemon-sour/lemon-sour.js/tree/master/packages/lemon-sour#readme",
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=prod rimraf lib/* && tsc",
    "watch": "cross-env NODE_ENV=dev tsc -w",
    "dev": "npm run build && cross-env NODE_ENV=prod node -r esm bin/lemon-sour --yml ../../example/app_basic_no_archive/index.yml",
    "node:watch": "cross-env NODE_ENV=dev nodemon bin/lemon-sour --yml ../../example/app_basic_archive/index.yml",
    "test": "cross-env NODE_ENV=prod jest",
    "test:coverage": "cross-env NODE_ENV=prod jest --no-cache --forceExit --coverage",
    "tsc:init": "tsc --init",
    "lint": "prettier --list-different \"{src,__tests__}/**/*.{js,ts,tsx,json}\"",
    "lint:format": "prettier --write \"{src,__tests__}/**/*.{js,ts,tsx,json}\"",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "linters": {
      "{src,__tests__}/**/*.{js,ts,tsx,json}": [
        "npm run lint:format",
        "git add"
      ]
    }
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": false,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ]
  },
  "bin": {
    "lemon-sour": "bin/lemon-sour"
  },
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib",
    "bin",
    "types"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lemon-sour/lemon-sour.js.git"
  },
  "keywords": [
    "deployment",
    "autoupdater"
  ],
  "bugs": {
    "url": "https://github.com/lemon-sour/lemon-sour.js/issues"
  },
  "gitHead": "b6ad574816588dd8328ca67b04347ee12d0e7076",
  "dependencies": {
    "@lemon-sour/cli": "^1.0.26",
    "@lemon-sour/json-storage": "^1.1.0",
    "@sentry/node": "4.5.3",
    "adm-zip": "^0.4.13",
    "axios": "^0.18.0",
    "eventemitter3": "^3.1.0",
    "follow-redirects": "^1.6.1",
    "js-yaml": "^3.12.1",
    "lint-staged": "^8.1.0",
    "lodash": "^4.17.11",
    "mkdirp": "^0.5.1",
    "mv": "^2.1.1",
    "node-fetch": "^2.3.0",
    "node-log-rotate": "^0.0.7",
    "razer": "^0.0.6",
    "rimraf": "^2.6.3"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.4.32",
    "@types/jest": "^23.3.12",
    "@types/js-yaml": "^3.11.4",
    "@types/mv": "^2.1.0",
    "@types/node": "^10.12.18",
    "@types/node-fetch": "^2.1.4",
    "@types/request": "^2.48.1",
    "@types/rimraf": "^2.0.2",
    "cross-env": "^5.2.0",
    "esm": "^3.0.84",
    "jest": "^23.6.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3",
    "ts-jest": "^23.10.5",
    "typescript": "^3.2.2"
  }
}
