{
  "name": "lemon-sour",
  "version": "1.0.10",
  "description": "> TODO: description",
  "author": "hisasann <hisasann25@gmail.com>",
  "homepage": "https://github.com/lemon-sour/lemon-sour.js/tree/master/packages/lemon-sour#readme",
  "license": "ISC",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "node -r esm lib/index.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "codecov": "codecov",
    "tsc:init": "tsc --init",
    "lint": "prettier --list-different \"{src,__tests__}/**/*.{js,ts,tsx,json}\"",
    "lint:format": "prettier --write \"{src,__tests__}/**/*.{js,ts,tsx,json}\"",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "linters": {
      "{src,__tests__}/**/*.{js,ts,tsx,json}": [
        "npm run lint:format",
        "git add"
      ]
    }
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "bin": {
    "lemon-sour": "bin/lemon-sour"
  },
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib",
    "bin"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lemon-sour/lemon-sour.js.git"
  },
  "bugs": {
    "url": "https://github.com/lemon-sour/lemon-sour.js/issues"
  },
  "gitHead": "b6ad574816588dd8328ca67b04347ee12d0e7076",
  "dependencies": {
    "@lemon-sour/cli": "^1.0.9",
    "lint-staged": "^8.1.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.12",
    "@types/node": "^10.12.18",
    "codecov": "^3.1.0",
    "esm": "^3.0.84",
    "jest": "^23.6.0",
    "prettier": "^1.15.3",
    "ts-jest": "^23.10.5",
    "typescript": "^3.2.2"
  }
}
